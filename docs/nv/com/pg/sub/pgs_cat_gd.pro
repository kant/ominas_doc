;==================================================================================
; pgs_cat_gd
;
;==================================================================================
function pgs_cat_gd, gd

 tags = tag_names(gd)
 ntags = n_elements(tags)

 for i=0, ntags-1 do $
       if(keyword_set(gd.(i))) then xds = append_array(xds, gd.(i))

 return, xds
end
;==================================================================================
